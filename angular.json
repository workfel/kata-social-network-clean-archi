{
  "version": 1,
  "projects": {
    "front-core": {
      "root": "libs/front-core",
      "sourceRoot": "libs/front-core/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/front-core/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/front-core"
          ],
          "options": {
            "jestConfig": "libs/front-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "social-network": {
      "root": "libs/social-network",
      "sourceRoot": "libs/social-network/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/social-network/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/social-network"
          ],
          "options": {
            "jestConfig": "libs/social-network/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "digikom": {
      "projectType": "application",
      "root": "apps/digikom",
      "sourceRoot": "apps/digikom/src",
      "prefix": "poc-clean-archi-state-management",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/digikom",
            "index": "apps/digikom/src/index.html",
            "main": "apps/digikom/src/main.ts",
            "polyfills": "apps/digikom/src/polyfills.ts",
            "tsConfig": "apps/digikom/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/digikom/src/favicon.ico",
              "apps/digikom/src/assets"
            ],
            "styles": [
              "apps/digikom/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/digikom/src/environments/environment.ts",
                  "with": "apps/digikom/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "digikom:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "digikom:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "digikom:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/digikom/src/**/*.ts",
              "apps/digikom/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/digikom"
          ],
          "options": {
            "jestConfig": "apps/digikom/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "digikom-e2e": {
      "root": "apps/digikom-e2e",
      "sourceRoot": "apps/digikom-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/digikom-e2e/cypress.json",
            "tsConfig": "apps/digikom-e2e/tsconfig.e2e.json",
            "devServerTarget": "digikom:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "digikom:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/digikom-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "social-network-apollo": {
      "root": "libs/social-network-apollo",
      "sourceRoot": "libs/social-network-apollo/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/social-network-apollo/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/social-network-apollo"
          ],
          "options": {
            "jestConfig": "libs/social-network-apollo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "front-core-apollo": {
      "root": "libs/front-core-apollo",
      "sourceRoot": "libs/front-core-apollo/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/front-core-apollo/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/front-core-apollo"
          ],
          "options": {
            "jestConfig": "libs/front-core-apollo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "social-network-core": {
      "root": "libs/social-network-core",
      "sourceRoot": "libs/social-network-core/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/social-network-core/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/social-network-core"
          ],
          "options": {
            "jestConfig": "libs/social-network-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "pets-survey"
}